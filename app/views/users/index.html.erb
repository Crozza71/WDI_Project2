<h1>All Users</h1>


<% provide(:title, 'All users') %>


<ul class="users">
  <% @users.each do |user| %>
  <li>
    <div class="row">
      <div class="col-xs-6 col-md-3">
        <a href="users/<%= user.id %>"" class="thumbnail">
          <%= image_tag user.profile_picture.thumbnail.url %>
        </a>
      </div>
    </div>  
  </li>
  <% end %>
</ul>



