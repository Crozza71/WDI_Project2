<h1>All Users</h1>


<% provide(:title, 'All users') %>

<div class="row">
  <ul>
    <li>
      <% @users.each do |user| %>
      <ul class="col-xs-12 col-s-6 col-md-3 tile">
        <li><a href="/users/<%= user.id %>"><%= image_tag user.profile_picture.url, width: "100", height: "100" %></a></li>
      </ul>
      <% end %>
    </li>
  </ul>
</div>


