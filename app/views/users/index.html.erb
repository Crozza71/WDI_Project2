<h1>All Users</h1>


<% provide(:title, 'All users') %>


<ul class="users">
  <% @users.each do |user| %>
  <li>

    <div class="pro_block">
      <div class="col-xs-6 col-md-3">
        <a href="users/<%= user.id %>"" class="thumbnail">
        <div class="profile_pic" style="background-image:url('<%= user.profile_picture.thumbnail.url %>');"></div>
        </a>
      </div>
    </div>  
  </li>
  <% end %>
</ul>



