<p id="notice"><%= notice %></p>

<div class="quotes">
  <h1>Listing Quotes</h1>

  <ul>
    <li>
      <% @quotes.each do |quote| %>
        <ul class="card">
          <li><p><%= quote.body %></p></li>
          <li><%= link_to (image_tag quote.culprit.profile_picture.url),user_show_path(quote.culprit) %></li>
        </ul>
      <% end %>    
    </li>
  </ul>  
</div>

